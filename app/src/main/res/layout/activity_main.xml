<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="1dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <Spinner
                android:id="@+id/spinner11"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:entries="@array/count"
                android:spinnerMode="dropdown" />

            <Spinner
                android:id="@+id/spinner12"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:entries="@array/count"
                android:spinnerMode="dropdown" />

            <Spinner
                android:id="@+id/spinner13"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:entries="@array/count"
                android:spinnerMode="dropdown" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:entries="@array/count"
            android:spinnerMode="dropdown">

            <Spinner
                android:id="@+id/spinner21"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:entries="@array/count"
                android:spinnerMode="dropdown" />

            <Spinner
                android:id="@+id/spinner22"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:entries="@array/count"
                android:spinnerMode="dropdown" />

            <Spinner
                android:id="@+id/spinner23"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:entries="@array/count"
                android:spinnerMode="dropdown" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:entries="@array/count"
            android:spinnerMode="dropdown">

            <Spinner
                android:id="@+id/spinner31"
                android:layout_width="50dp"
                android:layout_height="100dp"
                android:entries="@array/count"
                android:spinnerMode="dropdown" />

            <Spinner
                android:id="@+id/spinner32"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:entries="@array/count"
                android:spinnerMode="dropdown" />

            <Spinner
                android:id="@+id/spinner33"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:entries="@array/count"
                android:spinnerMode="dropdown" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:entries="@array/count"
            android:spinnerMode="dropdown">

            <TextView
                android:id="@+id/status"
                android:layout_width="118dp"
                android:layout_height="wrap_content" />
        </TableRow>

    </TableLayout>
</android.support.constraint.ConstraintLayout>